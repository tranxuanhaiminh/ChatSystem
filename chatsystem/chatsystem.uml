        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_iz1rs4B1EeyKEauMWc46FA" name="loadHistory" specification="_iyhdEIB1EeyKEauMWc46FA">
          <ownedParameter xmi:id="_iz1rtIB1EeyKEauMWc46FA" name="contact" type="_iuvwUIB1EeyKEauMWc46FA"/>
