        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_izwMIIB1EeyKEauMWc46FA" name="removeConv" specification="_iyBGwYB1EeyKEauMWc46FA">
          <ownedParameter xmi:id="_izwMIYB1EeyKEauMWc46FA" name="conv" type="_iu46QIB1EeyKEauMWc46FA"/>
          <language>java</language>
          <body>conv.close();&#xD;
		convList.remove(conv);&#xD;
		for (ChatWindow window : windowList) {&#xD;
			if (conv.getDest().getIpaddress().equals(window.getContact().getIpaddress())) {&#xD;
				window.setConv(null);&#xD;
				break;&#xD;
			}&#xD;
		}&#xD;
	</body>
